{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "module": "ES2015",
    "moduleResolution": "Node",
    "strict": true,
    "removeComments": true,
    "declaration": true,
    /* "esModuleInterop": true, */
    /* Advanced Options */
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
